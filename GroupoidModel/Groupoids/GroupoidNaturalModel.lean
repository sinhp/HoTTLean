instance groupoidULiftHom.{u'} {Œ± : Type u} [Groupoid.{v} Œ±] : Groupoid (ULiftHom.{u'} Œ±) where
  inv f := .up (Groupoid.inv f.down)
  inv_comp _ := ULift.ext _ _ <| Groupoid.inv_comp ..
